<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="calendar">

 <insert id="insertCalendar" parameterType="calendar">
      INSERT INTO CALENDAR VALUES(seq_calendar_no.nextval,#{partner_No},#{schedule},#{start_Date},#{end_Date},#{calendar_Content})
   </insert>
   
   <select id="selectCalendarOne" parameterType="calendar">
      SELECT * FROM CALENDAR WHERE PARTNER_NO=#{partner_No}
   
   </select>
   
   <select id="selectCalendar" parameterType="string" resultType="calendar">
     SELECT * FROM CALENDAR WHERE PARTNER_NO=#{partner_No}
   
   </select>




</mapper>